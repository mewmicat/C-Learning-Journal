#include <stdio.h>

int main() {
    int integer, sum;
    printf("Please enter your first integer: ");
    scanf("%d", &integer);
    sum = integer;
    printf("Please enter your second integer: ");
    scanf("%d", &integer);
    sum = integer + sum;
    printf("");
    scanf("");
    sum = integer + sum;
    printf("Sum is %d\n", sum);
    return 0;
}